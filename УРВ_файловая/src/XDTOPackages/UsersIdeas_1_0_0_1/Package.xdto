	</objectType>
	<objectType name="Attachment">
		<property name="Name" type="xs:string"/>
		<property name="Data" type="xs:base64Binary"/>
	</objectType>
	<objectType name="IdeaComment">
		<property name="UserName" type="xs:string"/>
		<property name="UserId" type="xs:string"/>
		<property name="Date" type="xs:dateTime"/>
		<property name="IsSupport" type="xs:boolean"/>
		<property name="Text" type="xs:string"/>
		<property xmlns:d3p1="http://www.1c.ru/1cFresh/InformationCenter/UsersIdeas/1.0.0.1" name="MainIdeaComment" type="d3p1:IdeaComment" nillable="true"/>
		<property name="Id" type="xs:string"/>
	</objectType>
	<objectType name="AttachmentList">
		<property xmlns:d3p1="http://www.1c.ru/1cFresh/InformationCenter/UsersIdeas/1.0.0.1" name="AttachmentElement" type="d3p1:Attachment" lowerBound="0" upperBound="-1"/>
