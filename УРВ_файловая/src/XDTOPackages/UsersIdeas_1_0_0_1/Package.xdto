		<property name="Rating" type="xs:int"/>
	</objectType>
	<objectType name="IdeaPresentation">
		<property xmlns:d3p1="http://www.1c.ru/1cFresh/InformationCenter/UsersIdeas/1.0.0.1" name="Idea" type="d3p1:Idea"/>
