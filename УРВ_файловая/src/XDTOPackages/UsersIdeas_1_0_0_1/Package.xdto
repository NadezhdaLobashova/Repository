<package xmlns="http://v8.1c.ru/8.1/xdto" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" targetNamespace="http://www.1c.ru/1cFresh/InformationCenter/UsersIdeas/1.0.0.1">
	<valueType name="StatusFilter" base="xs:string">
		<enumeration xsi:type="xs:string">plan</enumeration>
		<enumeration xsi:type="xs:string">voiting</enumeration>
		<enumeration xsi:type="xs:string">deviation</enumeration>
		<enumeration xsi:type="xs:string">realization</enumeration>
		<enumeration xsi:type="xs:string">favorites</enumeration>
		<enumeration xsi:type="xs:string">search</enumeration>
	</valueType>
	<valueType name="SortType" base="xs:string">
		<enumeration xsi:type="xs:string">PlanMadeDate</enumeration>
		<enumeration xsi:type="xs:string">CreateDate</enumeration>
		<enumeration xsi:type="xs:string">ClosingDate</enumeration>
		<enumeration xsi:type="xs:string">CommentsCount</enumeration>
		<enumeration xsi:type="xs:string">Rating</enumeration>
		<enumeration xsi:type="xs:string">ChangingDate</enumeration>
	</valueType>
	<objectType name="StatusFilterArray">
		<property xmlns:d3p1="http://www.1c.ru/1cFresh/InformationCenter/UsersIdeas/1.0.0.1" name="StatusFilterElement" type="d3p1:StatusFilter" lowerBound="0" upperBound="-1"/>
	</objectType>
	<objectType name="SubjectFilterArray">
		<property name="SubjectFilterElement" type="xs:string" lowerBound="0" upperBound="-1"/>
	</objectType>
	<objectType name="IdeaListPresentation">
		<property xmlns:d3p1="http://www.1c.ru/1cFresh/InformationCenter/UsersIdeas/1.0.0.1" name="IdeasList" type="d3p1:Idea" lowerBound="0" upperBound="-1"/>
		<property name="IdeasCount" type="xs:int"/>
		<property name="SubjectsList" type="xs:string" lowerBound="0" upperBound="-1"/>
	</objectType>
	<objectType name="Idea">
		<property name="Id" type="xs:string"/>
		<property name="Name" type="xs:string"/>
		<property name="UserId" type="xs:string"/>
		<property name="UserName" type="xs:string"/>
		<property name="CommentsCount" type="xs:int"/>
		<property name="HTMLText" type="xs:string"/>
