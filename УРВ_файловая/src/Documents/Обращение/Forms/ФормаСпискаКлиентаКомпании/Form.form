  <attributes>
    <name>Список</name>
    <title>
      <key>ru</key>
      <value>Список</value>
    </title>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	Документ.Ссылка КАК Ссылка,
	Документ.ПометкаУдаления КАК ПометкаУдаления,
	Документ.Номер КАК Номер,
	Документ.НомерСокращенный КАК НомерСокращенный,
	Документ.Дата КАК Дата,
	Документ.Проведен КАК Проведен,
	Документ.Инициатор КАК Инициатор,
	Документ.Исполнитель КАК Исполнитель,
	Документ.Состояние КАК Состояние,
	Документ.Тема КАК Тема,
	ВЫБОР
		КОГДА Документ.Важность = &amp;НизкаяВажность
			ТОГДА 0
		КОГДА Документ.Важность = &amp;ВысокаяВажность
			ТОГДА 2
		ИНАЧЕ 1
	КОНЕЦ КАК ВажностьКартинка,
	ВЫБОР
		КОГДА Документ.ТипОбращения = &amp;ТипКонсультация
			ТОГДА 0
		КОГДА Документ.ТипОбращения = &amp;ТипОшибка
			ТОГДА 1
		КОГДА Документ.ТипОбращения = &amp;ТипПожелание
			ТОГДА 2
		ИНАЧЕ 3
	КОНЕЦ КАК ТипОбращенияКартинка,
	Документ.Автор КАК Автор,
	Документ.Абонент КАК Абонент,
	Документ.Оценка КАК Оценка,
	Документ.ДатаЗакрытия КАК ДатаЗакрытия,
	Документ.ДатаИзменения КАК ДатаИзменения,
	ВЫБОР
		КОГДА Сроки.СрокОбработки = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, Сроки.СрокОбработки, СЕКУНДА)
	КОНЕЦ КАК Осталось
ИЗ
	Документ.Обращение КАК Документ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СрокиОбращенийПоСоглашениям.СрезПоследних КАК Сроки
		ПО (Сроки.Обращение = Документ.Ссылка)
ГДЕ
	НЕ Документ.ПометкаУдаления
	И ВЫБОР
			КОГДА &amp;СостоянияОбращений = &quot;Все&quot;
				ТОГДА ИСТИНА
			КОГДА &amp;СостоянияОбращений = &quot;Закрытые&quot;
				ТОГДА Документ.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияОбращений.Закрыто)
			КОГДА &amp;СостоянияОбращений = &quot;Открытые&quot;
				ТОГДА НЕ Документ.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияОбращений.Закрыто)
			КОГДА &amp;СостоянияОбращений = &quot;Ожидающие&quot;
				ТОГДА Документ.Состояние = ЗНАЧЕНИЕ(Перечисление.СостоянияОбращений.ОжиданиеИнициатора)
			ИНАЧЕ ЛОЖЬ
		КОНЕЦ</queryText>
      <mainTable>Document.Обращение</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
