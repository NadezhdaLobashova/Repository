#Область ПрограммныйИнтерфейс
    
// Возвращает префикс указанного сервиса.
//
Функция ПрефиксСервиса(Сервис) Экспорт
	
	Префикс = ОбщегоНазначения.ЗначениеРеквизитаОбъекта(Сервис, "Префикс");
	Если Не ЗначениеЗаполнено(Префикс)  Тогда
		Префикс = "00-";
	Иначе
		Префикс = Префикс + "-";
	КонецЕсли; 
	
	Возврат Префикс;
	
КонецФункции

#КонецОбласти 
