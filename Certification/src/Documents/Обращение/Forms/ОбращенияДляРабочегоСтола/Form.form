  <attributes>
    <name>Список</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Дата</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
    Документ.Ссылка,
    Документ.ПометкаУдаления,
    Документ.Номер,
    Документ.НомерСокращенный,
    Документ.Дата КАК Дата,
    Документ.Проведен,
    Документ.Инициатор,
    Документ.ОбслуживающаяОрганизация,
    Документ.Важность,
    Документ.Сервис,
    Документ.Витрина,
    Документ.Описание,
    Документ.Исполнитель,
    Документ.Состояние,
    Документ.Тема,
    Документ.ТипОбращения,
    Документ.Компонент,
    Документ.Раздел,
    ДОБАВИТЬКДАТЕ(Документ.Дата, СЕКУНДА, &amp;СекундДоМестногоВремени) КАК ДатаМестная,
    ЕСТЬNULL(Файлы.ЕстьФайлы, ЛОЖЬ) КАК ЕстьФайлы,
    ЕСТЬNULL(Обновления.ЕстьОбновление, ЛОЖЬ) КАК ЕстьОбновление,
    ЕСТЬNULL(Сроки.СрокРеакцииСорван, ЛОЖЬ) КАК СрокРеакцииСорван,
    ЕСТЬNULL(Сроки.СрокРеакцииСорван, ЛОЖЬ) КАК СрокОбработкиСорван,
    ЕСТЬNULL(Сроки.СрокОбработки, &amp;ПустаяДата) КАК СрокОбработки,
    ВЫБОР
        КОГДА Документ.Важность = &amp;НизкаяВажность
            ТОГДА 0
        КОГДА Документ.Важность = &amp;ВысокаяВажность
            ТОГДА 2
        ИНАЧЕ 1
    КОНЕЦ КАК ВажностьКартинка,
    ВЫБОР
        КОГДА Документ.ТипОбращения = &amp;ТипКонсультация
            ТОГДА 0
        КОГДА Документ.ТипОбращения = &amp;ТипОшибка
            ТОГДА 1
        КОГДА Документ.ТипОбращения = &amp;ТипПожелание
            ТОГДА 2
        ИНАЧЕ 3
    КОНЕЦ КАК ТипОбращенияКартинка,
    ВЫБОР
        КОГДА ЕСТЬNULL(Сроки.СрокОбработки, &amp;ПустаяДата) = &amp;ПустаяДата
            ТОГДА &amp;ПустаяДата
        ИНАЧЕ ДОБАВИТЬКДАТЕ(Сроки.СрокОбработки, СЕКУНДА, &amp;СекундДоМестногоВремени)
    КОНЕЦ КАК СрокОбработкиМестный,
    ВЫБОР
        КОГДА Сроки.СрокОбработки = ДАТАВРЕМЯ(1, 1, 1)
            ТОГДА 0
        ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, Сроки.СрокОбработки, СЕКУНДА)
    КОНЕЦ КАК Осталось,
    ВЫБОР
        КОГДА ЕСТЬNULL(Сроки.СрокОбработки, &amp;ПустаяДата) = &amp;ПустаяДата
            ТОГДА 0
        ИНАЧЕ РАЗНОСТЬДАТ(&amp;ТекущаяДата, Сроки.СрокОбработки, СЕКУНДА) / 3600
    КОНЕЦ КАК СрокВЧасах,
    ВЫБОР
        КОГДА ЕСТЬNULL(Сроки.СрокОбработкиСорван, ЛОЖЬ)
            ТОГДА 1
        КОГДА ЕСТЬNULL(Сроки.СрокРеакцииСорван, ЛОЖЬ)
            ТОГДА 3
        ИНАЧЕ 0
    КОНЕЦ КАК КартинкаСрока,
    ВЫБОР
        КОГДА ЕСТЬNULL(Рассмотрение.КоличествоНеРассмотрено, 0) > 0
            ТОГДА ИСТИНА
        ИНАЧЕ ЛОЖЬ
    КОНЕЦ КАК ЕстьНеРассмотренныеВзаимодействия
    //{Рарус kruser 02.04.2019 84062
	,Документ.Автор,
	//}Рарус kruser 02.04.2019 84062
	//{Рарус kruser 25.04.2019 84132
	Документ.ВнутреннееОбращение,
	//}Рарус kruser 25.04.2019 84132
	//{Рарус kruser 22.05.2019 86182
	Документ.ИмеетСвязьСВнутренними,
	Документ.ИмеетСвязьСВнешними
	//}Рарус kruser 22.05.2019 86182
ИЗ
    Документ.Обращение КАК Документ
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК Файлы
        ПО (Файлы.ОбъектСФайлами ССЫЛКА Документ.Обращение)
            И (Файлы.ОбъектСФайлами = Документ.Ссылка)
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеОбновленийОбращений КАК Обновления
        ПО Документ.Ссылка = Обновления.Обращение
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СрокиОбращенийПоСоглашениям.СрезПоследних КАК Сроки
        ПО Документ.Ссылка = Сроки.Обращение
        
        ЛЕВОЕ СОЕДИНЕНИЕ (
        ВЫБРАТЬ
        	ПредметыПапкиВзаимодействий.Предмет КАК Предмет,
			СУММА(ВЫБОР
					КОГДА ПредметыПапкиВзаимодействий.Рассмотрено
						ТОГДА 0
					ИНАЧЕ 1
				КОНЕЦ) КАК КоличествоНеРассмотрено
		ИЗ
			РегистрСведений.ПредметыПапкиВзаимодействий КАК ПредметыПапкиВзаимодействий
		ГДЕ
			ПредметыПапкиВзаимодействий.Предмет ССЫЛКА Документ.Обращение
		СГРУППИРОВАТЬ ПО
			ПредметыПапкиВзаимодействий.Предмет) КАК Рассмотрение
		ПО Рассмотрение.Предмет = Документ.Ссылка
ГДЕ
    НЕ Документ.ПометкаУдаления
    И Документ.Состояние &lt;> &amp;СостояниеЗакрыто
    И ВЫБОР
            КОГДА &amp;Вариант = &quot;Новые&quot;
                ТОГДА Документ.Состояние = &amp;СостояниеНовое
                        И НЕ Документ.Исполнитель ССЫЛКА Справочник.Пользователи
            КОГДА &amp;Вариант = &quot;НаМне&quot;
                ТОГДА Документ.Исполнитель = &amp;ТекущийПользователь
            КОГДА &amp;Вариант = &quot;ОбщиеНаЛинии&quot;
                ТОГДА Документ.Исполнитель = &amp;ЛинияПоддержки
                        И Документ.Состояние &lt;> &amp;СостояниеНовое
            КОГДА &amp;Вариант = &quot;ВРаботеУЛинии&quot;
                ТОГДА Документ.Исполнитель В (&amp;ПользователиЛинииПоддержки)
                        И Документ.Исполнитель &lt;> &amp;ТекущийПользователь
            КОГДА &amp;Вариант = &quot;Все&quot;
                ТОГДА ИСТИНА
        КОНЕЦ</queryText>
      <mainTable>Document.Обращение</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
