	втИтог.Проект КАК Проект,
	втИтог.ЭтапПроекта КАК ЭтапПроекта,
	втИтог.ЭтапЗакрыт КАК ЭтапЗакрыт,
	втИтог.СуммаЭтапа КАК СуммаЭтапа,
	втИтог.СуммаАктов КАК СуммаАктов,
	ВЫБОР
		КОГДА втИтог.СуммаЭтапа &gt; 0
			ТОГДА втИтог.СуммаАктов * 100 / втИтог.СуммаЭтапа
		ИНАЧЕ 0
	КОНЕЦ КАК ПроцентАктирования,
	втИтог.ОтработаноЧасов КАК ОтработаноЧасов,
	втИтог.ОтработаноСумма КАК ОтработаноСумма,
	втИтог.ВыставленныеЧасы КАК ВыставленныеЧасы,
	втИтог.ОтработаноСумма * 1.2 * 0.87 * 1.331 КАК СуммаЗатрат,
	втИтог.СуммаАктов * 0.3 - втИтог.ОтработаноСумма * 1.2 * 0.87 * 1.331 КАК ДоступныйФОТ,
	втИтог.ОтработаноСумма * 1.2 * 0.87 * 1.331 + втИтог.ВыставленныеСуммаОстаток * 1.2 * 0.87 * 1.331 КАК СуммаЗатратВсего,
	втИтог.СуммаАктов * 0.3 - втИтог.ОтработаноСумма * 1.2 * 0.87 * 1.331 + втИтог.ВыставленныеСуммаОстаток * 1.2 * 0.87 * 1.331 КАК ДоступныйФОТВсего,
	втИтог.Акт КАК Акт,
	втИтог.Сотрудник КАК Сотрудник
ИЗ
	втИтог КАК втИтог</query>
