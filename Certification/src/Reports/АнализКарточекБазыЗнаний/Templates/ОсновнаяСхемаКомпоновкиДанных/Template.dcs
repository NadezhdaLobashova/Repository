				<dcscom:required>true</dcscom:required>
			</role>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Консультации.Ссылка КАК Карточка,
	Консультации.Ссылка КАК КоличествоКарточек,
	ЕСТЬNULL(КонсультацииКомпоненты.Компонент, &amp;ПустойКомпонент) КАК Компонент,
	ЕСТЬNULL(КонсультацииРазделы.Раздел, &amp;ПустойРаздел) КАК Раздел,
	ЕСТЬNULL(КонсультацииСервисы.Сервис, &amp;ПустойСервис) КАК Сервис,
	&amp;ТипКарточкиКонсультация КАК ТипКарточки
ПОМЕСТИТЬ ВсеКарточки
ИЗ
	Справочник.Консультации КАК Консультации
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Консультации.Компоненты КАК КонсультацииКомпоненты
		ПО Консультации.Ссылка = КонсультацииКомпоненты.Ссылка}
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Консультации.Разделы КАК КонсультацииРазделы
		ПО Консультации.Ссылка = КонсультацииРазделы.Ссылка}
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Консультации.Сервисы КАК КонсультацииСервисы
		ПО Консультации.Ссылка = КонсультацииСервисы.Ссылка}
ГДЕ
	КонсультацииСервисы.Сервис = КонсультацииКомпоненты.Компонент.Владелец

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Ошибки.Ссылка,
	Ошибки.Ссылка,
	ЕСТЬNULL(ОшибкиКомпоненты.Компонент, &amp;ПустойКомпонент),
	ЕСТЬNULL(ОшибкиРазделы.Раздел, &amp;ПустойРаздел),
	ЕСТЬNULL(ОшибкиСервисы.Сервис, &amp;ПустойСервис),
	&amp;ТипКарточкиОшибка
ИЗ
	Справочник.Ошибки КАК Ошибки
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Ошибки.Компоненты КАК ОшибкиКомпоненты
		ПО Ошибки.Ссылка = ОшибкиКомпоненты.Ссылка}
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Ошибки.Разделы КАК ОшибкиРазделы
		ПО Ошибки.Ссылка = ОшибкиРазделы.Ссылка}
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Ошибки.Сервисы КАК ОшибкиСервисы
		ПО Ошибки.Ссылка = ОшибкиСервисы.Ссылка}
ГДЕ
	ОшибкиСервисы.Сервис = ОшибкиКомпоненты.Компонент.Владелец

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Пожелания.Ссылка,
	Пожелания.Ссылка,
	ЕСТЬNULL(ПожеланияКомпоненты.Компонент, &amp;ПустойКомпонент),
	ЕСТЬNULL(ПожеланияРазделы.Раздел, &amp;ПустойРаздел),
	ЕСТЬNULL(ПожеланияСервисы.Сервис, &amp;ПустойСервис),
	&amp;ТипКарточкиПожелание
ИЗ
	Справочник.Пожелания КАК Пожелания
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Пожелания.Компоненты КАК ПожеланияКомпоненты
		ПО Пожелания.Ссылка = ПожеланияКомпоненты.Ссылка}
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Пожелания.Разделы КАК ПожеланияРазделы
		ПО Пожелания.Ссылка = ПожеланияРазделы.Ссылка}
		{ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Пожелания.Сервисы КАК ПожеланияСервисы
		ПО Пожелания.Ссылка = ПожеланияСервисы.Ссылка}
ГДЕ
	ПожеланияСервисы.Сервис = ПожеланияКомпоненты.Компонент.Владелец
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВсеКарточки.Карточка,
	ВсеКарточки.КоличествоКарточек,
	ВсеКарточки.Компонент,
	ВЫБОР
		КОГДА РазделыКомпоненты.Компонент ЕСТЬ NULL 
			ТОГДА &amp;ПустойРаздел
		ИНАЧЕ ВсеКарточки.Раздел
	КОНЕЦ КАК Раздел,
	ВсеКарточки.Сервис,
	ВсеКарточки.ТипКарточки
ИЗ
	ВсеКарточки КАК ВсеКарточки
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Разделы.Компоненты КАК РазделыКомпоненты
		ПО ВсеКарточки.Раздел = РазделыКомпоненты.Ссылка
			И ВсеКарточки.Компонент = РазделыКомпоненты.Компонент
ГДЕ
	ВсеКарточки.Компонент = ВсеКарточки.Компонент.Ссылка
	И ВсеКарточки.Сервис = ВсеКарточки.Сервис.Ссылка</query>
